<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jetcemetery.androidcalculus">
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/calchashtag"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/calchashtag"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalculusPhoneNumber">

        <activity android:name="com.jetcemetery.calculusPhoneNumber.activity.AboutActivity"/>
        <activity android:name="com.jetcemetery.calculusPhoneNumber.activity.SettingsActivity" />
        <activity android:name="com.jetcemetery.calculusPhoneNumber.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>